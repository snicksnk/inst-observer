// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Bot {
  id       Int     @id @default(autoincrement())
  username String  @unique
  password String
  proxy    String
  hasError Boolean
  error    String?
  session  String?
}
